    "@types/lodash": "^4.14.141",
    "@types/lusca": "^1.6.1",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^12.7.8",
    "@types/nodemailer": "^6.2.1",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.44",
    "@types/supertest": "^2.0.8",
    "@types/underscore": "^1.11.3",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
